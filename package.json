{
  "name": "@serlo/serlo.org-cloudflare-worker",
  "version": "0.0.0",
  "bugs": {
    "url": "https://github.com/serlo/serlo.org-cloudflare-worker/issues"
  },
  "repository": "serlo/serlo.org-cloudflare-worker",
  "license": "Apache-2.0",
  "author": "Serlo Education e.V.",
  "scripts": {
    "build": "wrangler build",
    "dev": "wrangler dev --env dev",
    "format": "yarn _prettier --write",
    "lint": "npm-run-all --parallel lint:prettier lint:tsc",
    "lint:prettier": "yarn _prettier --check",
    "lint:tsc": "tsc --noEmit",
    "start": "start-storybook -p 6006",
    "test": "jest",
    "pacts": "jest --config jest-pacts.config.js",
    "pacts:publish": "ts-node scripts/publish-pacts",
    "wrangler:publish": "wrangler publish",
    "wrangler:maintenance:kv:get": "wrangler kv:key get --binding=MAINTENANCE_KV",
    "wrangler:maintenance:kv:put": "wrangler kv:key put --binding=MAINTENANCE_KV",
    "wrangler:maintenance:kv:delete": "wrangler kv:key delete --binding=MAINTENANCE_KV",
    "maintenance": "ts-node scripts/maintenance.ts",
    "_prettier": "prettier \"{{__tests__,src}/**/*,*}.{js,ts,tsx,json,md}\"",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "apollo-datasource-rest": "^0.8.0",
    "apollo-server-cloudflare": "^2.0.0",
    "graphql-parse-resolve-info": "^4.0.0",
    "luxon": "^1.0.0",
    "preact": "^10.0.0",
    "preact-render-to-string": "^5.0.0",
    "ramda": "^0.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@cloudflare/wrangler": "^1.0.0",
    "@pact-foundation/pact": "^9.0.0",
    "@storybook/preact": "^5.0.0",
    "@types/jest": "^25.0.0",
    "@types/luxon": "^1.0.0",
    "@types/node": "^12.0.0",
    "@types/ramda": "^0.26.0",
    "@types/rimraf": "^2.0.0",
    "apollo-server-testing": "^2.0.0",
    "babel-loader": "^8.0.0",
    "jest": "^25.0.0",
    "nodemon": "^2.0.0",
    "npm-run-all": "^4.0.0",
    "prettier": "^1.0.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^25.0.0",
    "ts-loader": "^6.0.0",
    "ts-node": "^8.0.0",
    "typescript": "^3.0.0",
    "webpack": "^4.0.0",
    "webpack-cli": "^3.0.0"
  },
  "engines": {
    "node": "^12.0.0"
  },
  "private": true
}
